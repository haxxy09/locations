{
  "name": "District",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "population": {
      "type": "number",
      "required": true
    },
    "tas": {
      "type": [
        "number"
      ],
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "districtTa": {
      "type": "hasMany",
      "model": "TA",
      "foreignKey": "districtId",
      "options": {
        "nestRemoting": true
      }
    },
    "zone": {
      "type": "belongsTo",
      "model": "zone",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
